{
	"extends": [
		"wikimedia/client/es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"mw": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"es-x/no-class-fields": 0,
		"max-len": 0,
		"es-x/no-async-functions": 0,
		"es-x/no-array-prototype-includes": 0,
		"no-jquery/variable-pattern": 0
	}
}
